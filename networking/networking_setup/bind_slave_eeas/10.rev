; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	localhost. root.localhost. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL

;@	IN	NS	localhost.
@	IN	NS	israel-ubuntu1010.ipgdomain.com.
@	IN	NS	israel-ubuntu1010-client.ipgdomain.com.

